{
  "manifest_version": 3,
  "name": "Wiki Game Solver",
  "short_name": "Wiki Game Solver",
  "version": "1.0",
  "description": "Run the Wiki Game Solver inside an extension popup.",
  "icons": {
    "72": "icons/icon-72x72.png",
    "96": "icons/icon-96x96.png",
    "128": "icons/icon-128x128.png",
    "144": "icons/icon-144x144.png",
    "192": "icons/icon-192x192.png",
    "384": "icons/icon-384x384.png",
    "512": "icons/icon-512x512.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "72": "icons/icon-72x72.png",
      "96": "icons/icon-96x96.png",
      "128": "icons/icon-128x128.png"
    },
    "default_title": "Open Wiki Game Solver"
  },
  "background": {
    "service_worker": "sw.js"
  },
  "permissions": [],
  "web_accessible_resources": [
    {
      "resources": [
        "main.css",
        "bundle.js",
        "52.bundle.js"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
